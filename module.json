{
	"id": "easy-notes",
	"title": "Easy Notes",
	"description": "A simple module which add a user specific area for note taking",
	"version": "1.0.1",
	"compatibility": {
		"minimum": 10,
		"verified": 10
	},
	"authors": [{
		"name": "djeval"
	}],
	"relationships": {
		"requires": [{
			"id": "colorsettings",
			"type": "module",
			"manifest": "https://raw.githubusercontent.com/ardittristan/VTTColorSettings/2.9.0/module.json",
			"compatibility": {
				"verified": "2.9.0"
			}
		}]
	},
	"scripts": [
		"scripts/easy-notes.js"
	],
	"styles": [
		"styles/easy-notes.css"
	],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "languages/en.json"
		},
		{
			"lang": "ja",
			"name": "日本語",
			"path": "languages/ja.json"
		}
	],
	"url": "https://github.com/djeval/easy-notes",
	"manifest": "https://raw.githubusercontent.com/djeval/easy-notes/main/module.json",
	"download": "https://github.com/djeval/easy-notes/archive/refs/heads/main.zip"
}
